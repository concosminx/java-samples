<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Suite" parallel="tests">
  <listeners>
    <listener class-name="demotest.Listeners"/>
  </listeners>
  <test name="Validating User login">
    <classes>
      <class name="demotest.HomePage"/>
    </classes>
  </test>
  <test name="Validating title in the Page">
    <classes>
      <class name="demotest.ValidateTitle"/>
    </classes>
  </test>
  <test name="App Testing">
    <classes>
      <class name="demotest.AppTest"/>
    </classes>
  </test>
  <test name="Validating navigation Bar">
    <classes>
      <class name="demotest.ValidateNavigationBar"/>
    </classes>
  </test>
</suite> 

